        {
          "id": "call_1",
          "type": "function",
          "function": {
            "name": "calculator",
            "arguments": "{\"expression\": \"25 * 37\"}"
          }
        }
      ]
    },
    {
      "role": "tool",
      "tool_call_id": "call_1",
      "name": "calculator",
      "content": "925"
    },
    {
      "role": "assistant",
      "content": "The result is 925."
    },
    {
      "role": "user",
      "content": "Now divide that by 5."
    }
